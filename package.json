{
  "name": "start-gulp-template",
  "version": "1.0.0",
  "description": "Start boilerplate for static markup",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "gulp",
    "build": "gulp build --production",
    "pugLint": "glob-exec \"./src/pages/**/*.pug\" -- \"pug-lint {{files.join(' ')}}\"",
    "styleLint": "stylelint \"./src/styles/**/*.scss\"",
    "styleLintFix": "stylelint \"./src/styles/**/*.scss\" --fix",
    "esLint": "eslint \"./src/scripts/**/*.js\"",
    "esLintFix": "eslint \"./src/scripts/**/*.js\" --fix",
    "test": "npm run pugLint && npm run styleLint && npm run esLint",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/SmValeriya/start-gulp-template.git"
  },
  "keywords": [
    "gulp",
    "pug",
    "scss"
  ],
  "author": "Smirnova",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SmValeriya/start-gulp-template/issues"
  },
  "homepage": "https://github.com/SmValeriya/start-gulp-template#readme",
  "devDependencies": {
    "browser-sync": "^2.29.3",
    "del": "^7.1.0",
    "eslint": "^8.55.0",
    "glob-exec": "^0.1.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^9.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-group-css-media-queries": "^1.2.2",
    "gulp-if": "^3.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-pug": "^5.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-rev": "^11.0.0",
    "gulp-rev-rewrite": "^6.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sass-glob": "^1.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "husky": "^8.0.3",
    "pug-lint": "^2.7.0",
    "sass": "^1.69.5",
    "stylelint": "^16.0.2",
    "stylelint-config-standard-scss": "^12.0.0",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.89.0",
    "webpack-stream": "^7.0.0"
  }
}
